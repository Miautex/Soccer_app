<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="group2.schoolproject.a02soccer">

    <uses-permission android:name="android.permission.VIBRATE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity" android:label= "Santner's großes Fußballabenteuer"
            android:theme="@style/AppTheme.NoActionBar">

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".LoginActivity"
            android:label="@string/app_name" />
        <activity
            android:name=".EditPlayerActivity"
            android:label="@string/app_name" />
        <activity
            android:name=".AddPlayerActivity"
            android:label="@string/app_name" />
        <activity
            android:name=".TeamManagmentActivity"
            android:label="@string/title_activity_team_managment" />
        <!-- android:theme="@style/AppTheme.NoActionBar"></activity> -->
        <activity
            android:name=".AddGameSelectPlayersActivity"
            android:label="@string/app_name" />
        <activity
            android:name=".AddGameEnterDataActivity"
            android:label="@string/title_activity_add_game_enter_data" />
        android:name="group2.schoolproject.a02soccer.TeamManagmentActivity"
        android:label="@string/title_activity_team_managment"/>
        <!-- android:theme="@style/AppTheme.NoActionBar"></activity> -->
    </application>

</manifest>